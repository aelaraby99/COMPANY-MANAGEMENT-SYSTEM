@model RoleViewModel

<div class="form-group mt-0">
    <label class="col-form-label" asp-for="Id"></label>
    <input placeholder="Employee Name" class="form-control" asp-for="Id" readonly />
    <span class="text-danger" asp-validation-for="Id"></span>
</div>

<div class="form-group mt-0">
    <label class="col-form-label" asp-for="RoleName"></label>
    <input placeholder="Employee Name" class="form-control" asp-for="RoleName" readonly />
    <span class="text-danger" asp-validation-for="RoleName"></span>
</div>
